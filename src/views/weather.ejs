<%- include("partials/header"); -%>

<div class="container">
      <div class="result">
        <% if(temp !== null)  {%>
          <h1 class="temp"><%= temp %></h1>
        <% } %>
  
        <div class="icon-des">
          <% if(icon !== null)  {%>
            <img class="icon" src="http://openweathermap.org/img/w/<%= icon %>.png" alt="">
          <% } %>
          <% if(des !== null)  {%>
            <h3><%= des %></h3>
          <% } %>
        </div>
  
        <% if(city !== null)  {%>
          <p class="city"><%= city %></p>
        <% } %>
        
      </div>
</div>

<%- include("partials/footer"); -%>
